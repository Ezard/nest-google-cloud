{
  "name": "nest-google-cloud",
  "version": "0.0.0",
  "description": "Google Cloud modules for Nest.js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ben Ezard",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/pubsub": "^1.4.1"
  },
  "devDependencies": {
    "@nestjs/common": "^6.11.5",
    "@nestjs/testing": "^6.11.5",
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.11.0",
    "rxjs": "^6.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".spec.ts",
      ".module.ts"
    ]
  }
}
